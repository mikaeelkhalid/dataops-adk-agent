# Example Docker Environment Configuration
# Copy this file to .env.docker and update with your actual values

# Google Cloud Configuration
GOOGLE_GENAI_USE_VERTEXAI=1
GOOGLE_CLOUD_PROJECT=your-project-id-here
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_CLOUD_STORAGE_BUCKET=gs://your-unique-bucket-name
AGENT_ENGINE_ID=projects/your-project-id/locations/us-central1/reasoningEngines/your-engine-id

# Streamlit Configuration
STREAMLIT_SERVER_PORT=8501
STREAMLIT_SERVER_ADDRESS=0.0.0.0
STREAMLIT_SERVER_HEADLESS=true
STREAMLIT_BROWSER_GATHER_USAGE_STATS=false

# Optional: Model configuration
# MODEL_AGENT=gemini-2.5-flash
# MODEL_TOOL=gemini-2.5-flash